generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id                      Int      @id @default(autoincrement())
  email                   String?  
  name                    String?
  phone                   String   @unique
  start_date_conversation DateTime @default(now())
  last_date_conversation  DateTime?
  pipeline_user           String   @default("NOVO")
}
